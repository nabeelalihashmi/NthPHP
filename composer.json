{
    "name": "nabeelalihashmi/nthphp",
    "description": "A fast, optimized PHP framework using PHP attributes, automatic routing of Blade views, and RedBeanPHP.",
    "type": "project",
    "require": {
        "nikic/fast-route": "^1.3",
        "eftec/bladeone": "^4.16",
        "gabordemooij/redbean": "dev-master",
        "php": "^8.1",
        "rakit/validation": "^1.4",
        "phpmailer/phpmailer": "^6.9",
        "hashids/hashids": "^5.0",
        "tracy/tracy": "^2.10",
        "delight-im/auth": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Framework\\": "framework/",
            "Cli\\": "cli/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "php -r \"file_exists('vendor/autoload.php') || exit('Install dependencies with composer install');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nabeel Ali",
            "email": "mail2nabeelali@gmail.com"
        }
    ]
}
